{
  "name": "assign-stream",
  "version": "0.0.0",
  "description": "Writable stream that assigns enumerable own properties of chunks to the target object",
  "repository": "shinnn/assign-stream",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/assign-stream/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "assign",
    "data",
    "object",
    "extend",
    "property",
    "properties",
    "stream",
    "writable"
  ],
  "dependencies": {
    "inherits": "^2.0.1",
    "object-assign": "^2.0.0",
    "readable-stream": "^1.0.33"
  },
  "devDependencies": {
    "eslint": "^0.10.1",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.8.1",
    "stream-array": "^1.0.1",
    "tap-spec": "^2.1.1",
    "tape": "^3.0.3"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "validateLineBreaks": "LF"
  }
}
